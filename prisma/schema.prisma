generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model parti {
  id       Int      @id @default(autoincrement())
  navn     String
  adresse  String
  postnr   String
  epost    String
  poststed poststed @relation(fields: [postnr], references: [postnr])
}

model poststed {
  postnr String  @id
  sted   String
  parti  parti[]
}
